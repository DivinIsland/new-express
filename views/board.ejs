<!DOCTYPE html>
<html>

<head>
  <title><%= title %></title>
  <%- include("./common/head.ejs") %>
  <link rel='stylesheet' href='/css/board.css' />
</head>

<body>
  <div class="jumbotron text-center">
    <h1><%= title %></h1>
  </div>
  <%- include("./common/header.ejs"); %>
  <div class="function_area">
    <a href="/board/write" class="btn btn-warning">Write</a>
    <button class="btn btn-danger" id="deleteBtn">Delete</button>
  </div>

  <div class="container">
    <div class="show_area" id="target">
      <div class="row index_line">
        <div class="col-md-1">Check</div>
        <div class="col-md-2 text-center">Author</div>
        <div class="col-md-3 text-center">Title</div>
        <div class="col-md-3 text-center">Content</div>
        <div class="col-md-3 text-center">Date</div>
      </div>
      <% boardList.map(item=>{ %>
      <div class="row line">
        <div class="col-md-1"><input type="checkbox" data-name="<%= item.contentSeq %>"></div>
        <div class="col-md-2 text-center"><%= item.author %></div>
        <div class="col-md-3 text-center"><%= item.title %></div>
        <div class="col-md-3 text-center"><%= item.content %></div>
        <div class="col-md-3 text-center"><%= item.stringCreateAt %></div>
      </div>
      <% }) %>
    </div>
  </div>
  </div>



  <%- include("./common/foot.ejs") %>
  <script src="/js/board.js"></script>
</body>

</html>